.transportProcess {
    padding: 30rpx 20rpx;
}

.title {
    margin: 20rpx 0;
    font-weight: 700;
    font-size: 46rpx;
}

.text {
    line-height: 1.6;
    margin: 20rpx 0;
    font-size: 30rpx;
    letter-spacing: 2rpx;
    color: #111010;
}

.date {
    text-align: right;
    color: #929192;
    font-size: 28rpx;
}

.transportBtn {
    background-color: #83a4ff;
    width: 300rpx;
    color: #fff;
    font-size: 28rpx;
    padding: 0;
}

.btnWrap {
    width: 100%;
    background-color: #537ef5;
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 30rpx 0;
}

.transportProcess:last-child {
    margin-bottom: 100rpx;
}