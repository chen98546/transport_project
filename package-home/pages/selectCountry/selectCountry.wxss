page {
    background-color: #f5f5f5;
    padding-bottom: 0;
}

.van-cell::after {
    width: 90%;
    height: 2rpx;
    background-color: #e9e9e9;
    position: absolute;
    bottom: 0;
    left: 50%;
    transform: translateX(-50%);
}

.van-index-bar__index {
    margin: 10rpx 10rpx 10rpx 0;
}

.van-search {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    padding: var(--search-padding 20rpx 24rpx) !important;
}



.van-index-bar {
    top: 110rpx;
}