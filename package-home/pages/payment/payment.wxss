
.warp{
    width: 93vw;
    margin: 40rpx auto;
    box-sizing: border-box;
    box-shadow: 0 5rpx 20rpx -2rpx rgba(0, 0, 0, 0.4);
    padding: 20rpx 30rpx;
}

.warehouse {
    display: flex;
    align-items: center;
    font-size: 30rpx;
}

.noticeIcon {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    background: #40b850;
    box-shadow: 0 5rpx 10rpx 3rpx rgba(0, 0, 0, 0.4);
    margin-right: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.noticeIcon2 {
    background-color: #83a4ff;
}

.warehouseInfo {
    display: flex;
    font-size: 28rpx;
    color: #101010;
    margin: 15rpx 0;
}

.warehouseInfoLeft {
    margin-right: 30rpx;
}

.warehouseInfoRight {
    display: flex;
    align-items: center;
}

.warehouseAddress {
    font-size: 22rpx;
    color: #9fa19f;
    line-height: 2;
}

.copyPhoneNumber {
    background-color: #d9f1dd;
    color: #40b850;
    font-size: 16rpx;
    display: inline-block;
    padding: 2rpx 4rpx;
    border-radius: 10rpx;
    margin-left: 10rpx;
}

.warehouseBox {
    flex: 1;
}

.flex {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    margin: 10rpx 0 20rpx;
}